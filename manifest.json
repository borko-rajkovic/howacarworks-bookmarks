{
  "name": "How a car works bookmarks",
  "description": "Bookmarks for the website howacarworks.com",
  "version": "0.0.0.1",
  "permissions": ["activeTab"],
  "browser_action": {
    "default_icon": {
      "16": "images/wrench16.png",
      "24": "images/wrench24.png",
      "32": "images/wrench32.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://www.howacarworks.com/video-course/watch*"],
      "js": ["firebase-app.js", "firebase-firestore.js", "main.js"],
      "css": ["styles.css", "toggle.css", "checkbox.css"]
    }
  ],
  "manifest_version": 2
}
